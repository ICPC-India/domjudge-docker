sudo curl -fsSL https://get.docker.com | bash
usermod -aG docker ubuntu
su - ubuntu
git clone git@github.com:ICPC-India/domjudge-docker.git 
cd domjudge-docker
export EMAIL=dev@excelrate.in
export TRAEFIK_DOMAIN=traefik.test.indiaicpc.in 
export HASHED_PASSWORD=$(openssl passwd -apr1) #enter traefik password here and save it 
echo $HASHED_PASSWORD > traefik_pass.txt
docker compose -f traefik-dc.yml up
export PORTAINER_DOMAIN=portainer.test.indiaicpc.in 
docker compose -f portainer-dc.yml up


